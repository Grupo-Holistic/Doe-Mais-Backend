@model Fiap.Hollistic.Web.Model.Hospital

<div class="form-group">
    <label asp-for="Nome"></label>
    <input asp-for="Nome" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Cnpj"></label>
    <input asp-for="Cnpj" class="form-control" />
</div>

<div class="form-group">
    <label asp-for="Endereco.Rua"></label>
    <input asp-for="Endereco.Rua" class="form-control" />

</div>
<div class="form-group">
    <label asp-for="Endereco.Numero"></label>
    <input asp-for="Endereco.Numero" class="form-control" />

</div>
<div class="form-group">
    <label asp-for="Endereco.Estado"></label>
    <select asp-for="Endereco.Estado" class="form-control" asp-items="@Html.GetEnumSelectList<Fiap.Hollistic.Web.Model.Estado>()">
        <option>Selecione</option>
    </select>
</div>
<div class="form-group">
    <label asp-for="Endereco.Cidade"></label>
    <input asp-for="Endereco.Cidade" class="form-control" />

</div>
<div class="form-group">
    <label asp-for="Endereco.Cep"></label>
    <input asp-for="Endereco.Cep" class="form-control" />

</div>
<div class="form-group">
    <label asp-for="Endereco.Latitude"></label>
    <input asp-for="Endereco.Latitude" class="form-control" />

</div>
<div class="form-group">
    <label asp-for="Endereco.Longitude"></label>
    <input asp-for="Endereco.Longitude" class="form-control" />

</div>

